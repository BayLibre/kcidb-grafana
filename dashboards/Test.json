          "rawSql": "SELECT\n  origin,\n  origin_id,\n  path,\n  description,\n  IF(start_time IS NULL, \"\", CAST(start_time AS STRING)) as start_time_str,\n  IF(duration is NULL, \"\", FORMAT_TIME(\"%H:%M:%E3S\", TIME_ADD(TIME \"00:00:00\", INTERVAL CAST((duration * 1000) AS INT64) MILLISECOND))) as duration_str,\n  (CASE status WHEN \"SKIP\" THEN 1 WHEN \"DONE\" THEN 2 WHEN \"PASS\" THEN 3 WHEN \"FAIL\" THEN 4 WHEN \"ERROR\" THEN 5 ELSE 0 END) - (CASE waived WHEN false THEN 0 WHEN true THEN 1 ELSE 2 END) * 6 as status_num,\n  environment.description AS environment_description,\n  environment.misc AS environment_misc,\n  misc\nFROM kernelci02.tests\nWHERE origin = \"${origin}\" AND origin_id = \"${origin_id}\"\nORDER BY start_time DESC\nLIMIT 1\n",
